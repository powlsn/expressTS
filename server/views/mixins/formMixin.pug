mixin formMixin(obj)
    form(action!=attributes.action method="POST" class="container")
      input(id="data-id" class="form-control" type="hidden" name="userid" value=`${obj.user.id}`)
      input(id="data-id" class="form-control" type="hidden" name="photoid" value=`${obj.photo.id}`)

      .form-row
        .form-group.col-md-4.mx-auto
          label(for="firstname") Url
          input(id="firstname" class="form-control" type="text" name="imageUrl" aria-description="imageurlHelp" value=`${obj.photo.imageUrl}`)

      .form-row
        .form-group.col-md-4.offset-md-4
          button(class=`btn ${obj.button.class}` type="submit") #{obj.button.value}